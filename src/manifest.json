{
	"manifest_version": 2,

	"name": "Encryption to Magic",
	"description": "Replaces the text 'Encryption' with 'Magic'.",
	"author": "Matthew Potter",
	"version": "2.0",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
	"content_scripts":
	[
		{
			"matches": ["*://*/*"],
			"all_frames": true,
			"js": ["content_script.js"],
			"run_at": "document_end"
		}
	]
}
